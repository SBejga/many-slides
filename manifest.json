{
    "manifest_version": 2,
    "name": "Many-Slides",
    "version": "2.1",
    "minimum_chrome_version": "23",
    "offline_enabled": true,
    "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
    },
    "app": {
        "background": {
            "scripts": ["runtime.js"]
        }
    },
    "permissions": [
        "clipboardWrite",
        "storage"
    ]
}
